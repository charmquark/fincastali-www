<% content_for :head_inject do -%>
    <script src="http://crypto-js.googlecode.com/svn/tags/3.1.2/build/rollups/md5.js"></script>
<% end -%>

<section>
    <p class="big">You must log in to continue.</p>

    <%= form_tag admin_process_login_path do %>
        <div class="field">
            <%= label_tag :password %>
            <%= password_field_tag :password %>
        </div>
        <div class="actions">
            <%= submit_tag 'Login' %>
        </div>
    <% end -%>
</section>